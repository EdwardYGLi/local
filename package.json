{
  "name": "local",
  "version": "0.9.1",
  "description": "W&B Local is the self hosted version of Weights & Biases",
  "repository": "git@github.com:wandb/local.git",
  "author": "Chris Van Pelt <vanpelt@wandb.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@grupoboticario/news-fragments": "^1.7.0",
    "release-it": "^14.1.0"
  },
  "scripts": {
    "release": "release-it"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false,
      "release": false
    },
    "plugins": {
      "@grupoboticario/news-fragments": {
        "fragmentsTypes": [
          {
            "title": "Features",
            "extension": "feature"
          },
          {
            "title": "Bugfixes",
            "extension": "bugfix"
          }
        ]
      }
    }
  }
}
